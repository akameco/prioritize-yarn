{
  "name": "prioritize-yarn",
  "version": "1.0.1",
  "description": "If the project has yarn.lock, change npm install to yarn.",
  "license": "MIT",
  "repository": "akameco/prioritize-yarn",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "lint": "eslint .",
    "test": "npm run lint"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "cli-app",
    "cli",
    "yarn",
    "npm",
    "install"
  ],
  "dependencies": {
    "execa": "^1.0.0",
    "has-yarn": "^1.0.0",
    "meow": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.1.1",
    "eslint": "^5.13.0",
    "eslint-config-precure": "^4.3.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
