{
  "name": "prioritize-yarn",
  "version": "0.5.0",
  "description": "If the project has yarn.lock, change npm install to yarn.",
  "license": "MIT",
  "repository": "akameco/prioritize-yarn",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "akameco.github.io"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "precommit": "lint-staged",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "lint": "eslint .",
    "test": "npm run lint"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "lint-staged": {
    "*.js": [
      "fmt",
      "test",
      "git add"
    ],
    "*.json": [
      "fmt",
      "git add"
    ]
  },
  "keywords": [
    "cli-app",
    "cli",
    "yarn",
    "npm",
    "install"
  ],
  "dependencies": {
    "execa": "^0.8.0",
    "has-yarn": "^1.0.0",
    "meow": "^4.0.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.10.1",
    "eslint": "^4.15.0",
    "eslint-config-precure": "^0.13.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  }
}
